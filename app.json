{
  "expo": {
    "name": "vault_app",
    "slug": "vault_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/lock.png",
    "scheme": "vaultapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/lock.png",
        "backgroundImage": "./assets/images/lock.png",
        "monochromeImage": "./assets/images/lock.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sahil_master.vault_app"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/lock.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/lock.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#17618C",
          "dark": {
            "backgroundColor": "#17618C"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36c20e18-d0fc-4e9d-9b0e-ef331fef104a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/36c20e18-d0fc-4e9d-9b0e-ef331fef104a"
    }
  }
}
